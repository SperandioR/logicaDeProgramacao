Algoritmo "Teste_Saida"

Var
         idade: Inteiro
         salario: real
         nome: caractere


Inicio
       idade <- 32 // a <- siginifica que vai receber uma variavel X.
       salario <- 4560.9
       nome <- "Maria Silva"
       
       escreval("A funionaria ", nome, "ganha ", salario:8:2, " e tem ", idade, " anos")

Fimalgoritmo
