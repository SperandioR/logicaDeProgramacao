Algoritmo "Ex_glicose"

Var

   glicose : real

Inicio

   escreva("Digite a medida da glicose: ")
   leia(glicose)

   se glicose <= 100.0 entao
      escreval("Normal")
   senao
      se glicose <= 140.0 entao
         escreval("Elevado")
      senao
         escreval("Diabetes")
      fimse
   fimse

Fimalgoritmo

//Classificação Glicose
//Normal Até 100 mg/dl
//Elevado
//Maior que 100 até
//140 mg/dl
//Diabetes Maior de 140 mg/dl
